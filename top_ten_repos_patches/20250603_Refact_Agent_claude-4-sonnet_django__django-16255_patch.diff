+    def test_empty_sitemap_with_callable_lastmod(self):
+        """
+        Test that empty sitemaps with callable lastmod don't raise ValueError.
+        Regression test for issue where max() on empty sequence raised ValueError.
+        """
+        from datetime import datetime
+        
+        class EmptySitemapWithCallableLastmod(Sitemap):
+            def items(self):
+                return []
+            
+            def lastmod(self, item):
+                return datetime(2023, 1, 1, 12, 0, 0)
+        
+        sitemap = EmptySitemapWithCallableLastmod()
+        # This should not raise ValueError
+        result = sitemap.get_latest_lastmod()
+        self.assertIsNone(result)
+
+    def test_empty_sitemap_with_callable_lastmod_integration(self):
+        """
