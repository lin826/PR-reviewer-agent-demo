+            value = conn_params.get(param) or conn_params.get('options', {}).get(param)
+
+            if value:
+                subprocess_env[env_var] = str(value)
+
         try:
             # Allow SIGINT to pass to psql to abort queries.
             signal.signal(signal.SIGINT, signal.SIG_IGN)
