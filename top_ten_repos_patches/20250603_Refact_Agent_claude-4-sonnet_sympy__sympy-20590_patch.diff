+
+def test_symbol_no_dict():
+    """Test that Symbol instances do not have __dict__ when using __slots__."""
+    # This is a regression test for issue where Symbol instances
+    # incorrectly had __dict__ due to missing __slots__ in parent classes
+    s = Symbol('s')
+    
+    # Symbol should not have __dict__ when using __slots__
+    raises(AttributeError, lambda: s.__dict__)
+    
+    # Verify that Symbol still has __slots__
+    assert hasattr(s, '__slots__')
+    assert s.__slots__ == ('name',)
